{
  "name": "docs-github-com-api-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for docs.github.com API integration",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsc --watch & nodemon dist/index.js",
    "test": "node validation/test-mcp-client.js",
    "validate": "node validation/validate-simple.js",
    "validate-bash": "bash validation/validate-bash.sh",
    "docker:build": "docker build -f docker/Dockerfile -t mcp-server-template .",
    "docker:run": "docker run -p 3000:3000 --env-file .env mcp-server-template",
    "docker:compose": "docker-compose -f docker/docker-compose.yml up"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0",
    "nodemon": "^3.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "api-integration",
    "docs-github-com",
    "typescript"
  ],
  "author": "Your Name",
  "license": "MIT"
}