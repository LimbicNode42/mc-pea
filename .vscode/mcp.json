{
  "servers": {
    "github-local": {
      "command": "d:/HobbyProjects/mc-pea/github-mcp-server.exe",
      "args": ["stdio"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${GITHUB_PERSONAL_ACCESS_TOKEN}"
      }
    },    
    "filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "D:\\HobbyProjects"
      ]
    },    
    "auth": {
      "type": "http",
      "url": "${AUTH_MCP_URL}",
      "headers": {
        "mcp-session-id": "random-uuid",
        "API_KEY": "${AUTH_MCP_API_KEY}"
      }
    },    
    "db": {
      "type": "http",
      "url": "${DB_MCP_URL}",
      "headers": {
        "mcp-session-id": "random-uuid",
        "API_KEY": "${DB_MCP_API_KEY}"
      }
    }
  }
}
